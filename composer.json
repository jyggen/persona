{
    "name": "jyggen/persona",
    "type": "library",
    "description": "A framework agnostic implementation of Mozilla Persona, the cross-browser login system for the Web that's easy to use and easy to deploy.",
    "keywords": ["Mozilla", "Persona", "Mozilla Persona", "BrowserID", "Auth", "Authentication", "Login", "Laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "dev-master",
        "orchestra/testbench": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Jyggen\\Persona": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
