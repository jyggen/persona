    "type": "library",
    "keywords": ["Mozilla", "Persona", "Mozilla Persona", "BrowserID", "Auth", "Authentication", "Login", "Laravel"],
    "homepage": "https://github.com/jyggen/persona",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com",
            "homepage": "https://jyggen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jyggen/persona/issues",
        "source": "https://github.com/jyggen/persona"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "apigen/apigen": "~4.0",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "phpunit/phpunit": "~4.0",
